{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": ""
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "referenceTitle": "oci-node-sdk",
        "disableSort": false,
        "collapse": false,
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts" : {
        "template": "./jsdoc-template",
        "destination": "./docs/"
    }
}
